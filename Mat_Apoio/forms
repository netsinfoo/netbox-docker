class RespForm(BootstrapMixin, forms.ModelForm):

    class Meta:
        model = Resp
        fields = [
            'name',
            'tipo',
            'dom',
        ]

class RespFilterForm(BootstrapMixin, forms.ModelForm):
    q = forms.CharField(
        required=False,
        label="Search",
    )

    class Meta:
        model = Resp
        fields = [
            'name',
            'tipo',
            'dom',
        ]
